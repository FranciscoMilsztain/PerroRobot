//Milsztain, Macia, Colombres, Reboratti

#include <Wire.h>
#include <Adafruit_Sensor.h>
#include <Adafruit_BMP280.h>

Adafruit_BMP280 bmp;
float lecpres;

void setup() 
{
  Serial.begin(115200);
  bmp.begin(0x76);
}

void loop() 
{
  lecpres = bmp.readPressure(); //Se lee la presion en Pascales

  Serial.print("Lectura presion: ");
  Serial.println(lecpres);
}
