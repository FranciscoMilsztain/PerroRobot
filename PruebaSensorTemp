//PRUEBA TEMPERATURA

#include <Wire.h>
#include <Adafruit_AHTX0.h>

Adafruit_AHTX0 aht;
float lectemp;
float lechum;

void setup() 
{
  Serial.begin(115200);

  //inicio del AHT
  aht.begin()
}

void loop() 
{
  //Sensor Temp
  sensors_event_t humidity, temp;
  aht.getEvent(&humidity, &temp);
  
  lectemp = temp.temperature; //Lectura Temp (CÂ°)
  lechum = humidity.relative_humidity; //Lectura Humedad (%)
  
  Serial.print("Lectura temp: ");
  Serial.println(lectemp);

  Serial.print("Lectura hum: ");
  Serial.println(lechum);
}
